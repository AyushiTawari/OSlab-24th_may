def sort(l:list):
    for i in range(len(l)):
        for j in range(i + 1, len(l)):

            if l[i] > l[j]:
               l[i], l[j] = l[j], l[i]
    return (l)
x=int(input("No of inputs"))
l=[]
for i in range(x):
    num=int(input("Enter number"))
    l+=[num]
print("Orginal list is ",l)
x=sort(l)
print(l)       
